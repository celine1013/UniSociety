<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.celine.unisociety">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogIn_Activity" />
        <activity android:name=".Sign_Up_Activity" />
        <activity android:name=".CreatePost_Activity" />
        <activity android:name=".PostDetail_Activity" />
        <activity android:name=".PostHistoryActivity" />
        <activity android:name=".EventListByCategory_Activity"></activity>
    </application>

</manifest>